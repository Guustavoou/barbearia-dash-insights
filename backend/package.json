{
  "name": "unclic-backend",
  "version": "1.0.0",
  "description": "Backend API for Unclic salon management system",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:neon": "tsx watch src/neon-server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:neon": "node dist/neon-server.js",
    "migrate": "tsx src/database/migrate.ts",
